                                            <con:path>$body/ErrorHandlerRequest/MessageDetails/Message_TransactionId/text()</con:path>
                                        </con:param>
                                        <con:param name="Destination">
                                            <con:path>$body/ErrorHandlerRequest/MessageDetails/Destination/text()</con:path>
