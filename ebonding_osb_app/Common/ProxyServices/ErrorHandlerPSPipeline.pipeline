                        <con4:logLevel>warning</con4:logLevel>
                        <con4:expr>
                            <con2:xqueryText>$body/*</con2:xqueryText>
                        </con4:expr>
                        <con4:message>&lt;&lt; Error Handler PS Starts>></con4:message>
