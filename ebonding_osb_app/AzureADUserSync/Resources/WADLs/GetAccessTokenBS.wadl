<application xmlns:soa="http://www.oracle.com/soa/rest" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:tns="http://www.sita.aero/GetAccessTokenBS_GetToken_request" xmlns="http://wadl.dev.java.net/2009/02">
   <doc title="GetAccessTokenBS">RestReference</doc>
   <grammars>
      <xsd:schema>
            <xsd:import namespace="http://www.sita.aero/GetAccessTokenBS_GetToken_request" schemaLocation="../Schemas/GetAccessTokenRequest.xsd"/>
        </xsd:schema>
      <xsd:schema>
            <xsd:import namespace="http://www.sita.aero/GetAccessTokenBS_GetToken_response" schemaLocation="../Schemas/GetAccessTokenResponse.xsd"/>
        </xsd:schema>
   </grammars>
   <resources>
      <resource path="/{tenantId}/oauth2/token">
         <method name="POST" soa:wsdlOperation="GetToken">
            <request>
               <param name="tenantId" style="template" soa:expression="$property.tenantId" type="xsd:string"/>
               <representation mediaType="application/x-www-form-urlencoded" element="cns:TokenRequest" xmlns:cns="http://www.sita.aero/GetAccessTokenBS_GetToken_request"/>
            </request>
            <response status="200">
               <representation mediaType="application/json" element="cns:TokenResponse" xmlns:cns="http://www.sita.aero/GetAccessTokenBS_GetToken_response"/>
            </response>
         </method>
      </resource>
   </resources>
</application>