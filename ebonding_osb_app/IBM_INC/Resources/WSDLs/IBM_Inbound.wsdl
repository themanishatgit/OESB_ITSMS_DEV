                  targetNamespace="http://tempuri.org/"
                  xmlns:xs="http://www.w3.org/2001/XMLSchema" >
  <wsdl:types>
     <xs:schema elementFormDefault="qualified"  >
      <xs:element name="CallData">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="1" ref="sita"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:schema>
    <xs:schema elementFormDefault="qualified"  >
      <xs:element name="sita" >
        <xs:complexType>
          <xs:sequence>
            <xs:element name="recordtype" type="xs:string"/>
            <xs:element name="servicenownumber" type="xs:string"/>
            <xs:element name="servicenowstate" type="xs:string"/>
            <xs:element name="shortdescription" type="xs:string"/>
