            <xs:element name="category" type="xs:string"/>
            <xs:element name="ci" type="xs:string"/>
            <xs:element name="ibmLocationid" type="xs:string"/>
            <xs:element name="location" type="xs:string"/>
            <xs:element name="servicetype" type="xs:string"/>
            <xs:element name="serviceauthorization" type="xs:string"/>
            <xs:element name="serviceponumber" type="xs:string"/>
            <xs:element name="requiredonsite" type="xs:string"/>
            <xs:element name="servicenowdetails" type="xs:string"/>
          </xs:sequence>
          <xs:attribute name="transaction"/>
        </xs:complexType>
      </xs:element>

  <xs:element name="RESPONSE" >
		<xs:complexType>
			<xs:sequence>
				<xs:element name="STATUS" type="xs:string" minOccurs="1" maxOccurs="1"/>
				
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
  </wsdl:types>
  <wsdl:message name="ProcessIncidentSoapIn">
    <wsdl:part name="parameters" element="CallData"/>
  </wsdl:message>
  <wsdl:message name="ProcessIncidentSoapOut">
    <wsdl:part name="parameters" element="CallData"/>
  </wsdl:message>
  <wsdl:portType name="ServiceSoap">
    <wsdl:operation name="ProcessIncident">
      <wsdl:input message="tns:ProcessIncidentSoapIn"/>
      <wsdl:output message="tns:ProcessIncidentSoapOut"/>
    </wsdl:operation>
  </wsdl:portType>
  <wsdl:binding name="ServiceSoap" type="tns:ServiceSoap">
    <soap:binding transport="http://schemas.xmlsoap.org/soap/http"/>
    <wsdl:operation name="ProcessIncident">
      <soap:operation soapAction="http://tempuri.org/ProcessIncident" style="document"/>
      <wsdl:input>
        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output>
        <soap:body use="literal"/>
      </wsdl:output>
    </wsdl:operation>
  </wsdl:binding>
